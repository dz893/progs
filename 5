import java.util.Scanner;
public class Program5 {
	public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
		String c=new String();
		int n, i, k=0;
		c=sc.nextLine();
		n=c.length();
		char[] c1 = new char[n];
		c.getChars(0, n, c1, 0);
		for (i=0; i<n-1; i++) {
			if (c1[i]!=c1[n-i-1])
				k++;
		}
		if (k==0)
			System.out.print ("palindrom");
		else

			System.out.print ("ne palindrom");
		sc.close();
	}
}
